<!--==========================--><!--=         Banner         =--><!--==========================--><section class="banner banner-one">    <div class="circle-shape" data-parallax='{"y" : 230}'>        {% include 'mixins/img.twig' with { 'src': '/resource/img/header/circle-shape.png','alt': 'circle' } only %}    </div>    <div class="container">        <div class="banner-content-wrap">            <div class="row align-items-center">                <div class="col-lg-6">                    <div class="banner-content">                        <h1 class="banner-title wow pixFadeUp" data-wow-delay="0.3s">                            <span>Бесплатная</span>                            платформа для сайта                        </h1>                        <p class="description wow pixFadeUp" data-wow-delay="0.5s">                            Это всё что вам нужно чтобы запустить свой бизнес.                        </p>                        <a href="https://demo.getwebspace.org" class="pxs-btn banner-btn wow pixFadeUp" data-wow-delay="0.6s" target="_blank">                            Смотреть Демо <i class="fa fa-external-link-alt"></i>                        </a>                    </div><!-- /.banner-content -->                </div><!-- /.col-lg-6 -->                <div class="col-lg-6">                    <div class="promo-mockup wow pixFadeLeft">                        {% include 'mixins/img.twig' with { 'src': '/resource/img/header/macbook.webp','height': '539','width': '1000','alt': 'mpckup' } only %}                    </div><!-- /.promo-mockup -->                </div><!-- /.col-lg-6 -->            </div><!-- /.row -->        </div><!-- /.banner-content-wrap -->    </div><!-- /.container -->    <div class="bg-shape">        {% include 'mixins/img.twig' with { 'src': '/resource/img/header/shape-bg.png','alt': 'shape' } only %}    </div></section><!-- /.banner banner-one -->