{#    src - ссылка на картинку    id - attr id    class - attr class    width - width px    height - height px    alt - attr alt    data - data attr    title - attr title    style - attr style#}{% htmlcompress %}    {% if data %}        {% set buf = '' %}        {% for key, value in data %}            {% set buf = buf ~ ' data-' ~ key ~ '="' ~ value ~  '"' %}        {% endfor %}        {% set data = buf|trim %}    {% endif %}    <img data-lazyload-image data-src="{{ src }}" {{ id ? ('id="'~ id|trim ~ '"')|raw : '' }} {{ class ? ('class="'~ class|trim ~'"')|raw : '' }} {{ width ? ('width="' ~ width ~ '"')|raw : '' }} {{ height ? ('height="' ~ height ~ '"')|raw : '' }} {{ data|raw }} {{ alt ? ('alt="'~ alt|trim ~'"')|raw : '' }} {{ title ? ('title="'~ title|trim ~'"')|raw : '' }} {{ style ? ('style="'~ style|trim ~'"')|raw : '' }} />{% endhtmlcompress %}